	<artifactId>YX-Account-Service</artifactId>

	<packaging>war</packaging>

	<properties>
		<server.port>5050</server.port>
	</properties>

	<!-- 生成model -->
	<!-- mybatis-generator:generate -->

	<dependencies>
		<dependency>
			<groupId>com.geek.yx</groupId>
			<artifactId>YX-Common</artifactId>
			<version>${project.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.geek.yx</groupId>
			<artifactId>YX-User-Facade</artifactId>
